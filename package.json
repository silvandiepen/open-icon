{
  "name": "open-icon",
  "version": "1.0.0-alpha.0.24",
  "description": "Open Icon",
  "main": "lib/icons/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./scss":{
      "require": "./src/index.scss",
      "import": "./src/index.scss"
    },
    "./css":{
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "test": "npm run test",
    "build": "npm run build:cleanup && npm run build:icons && npm run build:lib && npm run build:dts && npm run build:style",
    "build:cleanup": "rm -rf dist/* && rm -rf lib/*",
    "build:lib": "tsup lib/index.ts  --format esm,cjs,iife",
    "build:style": "sass src/style/index.scss dist/style.css",
    "build:dts": "tsc ",
    "build:icons": "sh build/icon-components.sh"
  },
  "keywords": [
    "Open",
    "Icon"
  ],
  "author": "Sil van Diepen",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.77.7",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  }
}
